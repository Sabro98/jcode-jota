{
  "name": "jcode-jota",
  "displayName": "jcode-jota",
  "description": "submit code to jota",
  "version": "0.0.1",
  "publisher": "Sabro98",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sabro98/jcode-jota"
  },
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jcode-jota.submitCode",
    "onView:jcode.submissionView"
  ],
  "main": "./out/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "jcode.submissionView",
          "name": "Submit Code"
        }
      ]
    },
    "commands": [
      {
        "command": "jcode-jota.submitCode",
        "title": "Submit Code!!"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "node-fetch": "^2.6.1"
  }
}
